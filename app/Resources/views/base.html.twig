    {% block body %}{% endblock %}
    {% block javascripts %}
    <!-- BLOCK JAVASCRIPTS -->
    <!--[if lt IE 9]>
      <script src='http://html5shiv.googlecode.com/svn/trunk/html5.js'></script>
    <![endif]-->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.8.2.min.js"><\/script>')</script>
    <script src="{{ asset('/includes/boilerplate/js/plugins.js') }}"></script>
    <script src="{{ asset('/includes/jquery-ui-1.9.1/js/jquery-ui-1.9.1.custom.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/includes/boilerplate/js/modernizr-2.5.3.min.js') }}"></script>
    <script src="{{ asset('/includes/fancybox2/source/jquery.fancybox.pack.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/js/front.js') }}" type="text/javascript"></script>
    <script>
        var _gaq=[['_setAccount','UA-XXXXX-X'],['_trackPageview']];
